<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="GUI Tests suite" parallel="classes" thread-count="4">
    <test name="gui">
        <groups>
            <run>
                <include name="gui"/>
            </run>
        </groups>
        <packages>
            <package name="guitests.*"/>
        </packages>
    </test>
</suite>